{
  "presets": ["babel-preset-expo"],
  "plugins": [
    "react-native-reanimated/plugin",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./",
          "@components": "./components",
          "@screens": "./screens",
          "@utils": "./utils",
          "@context": "./context",
          "@assets": "./assets"
        }
      }
    ]
  ]
}